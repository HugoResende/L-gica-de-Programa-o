algoritmo "Creuza01"
var

    ano_atual: Inteiro
    ano_nasc:  Inteiro
    idade:     Inteiro
    
inicio
      Escreva("Em que ano nós estamos? ")
      Leia (ano_atual)
      Escreva("Em que ano nós estamos? ")
      Leia (ano_nasc)
      idade<- ano_atual - ano_nasc
      Escreva (idade)
 
fimalgoritmo
