algoritmo "semnome"
var
     ano, nasc, idade: Inteiro
inicio
      Escreva ("Em que ano estamos? ")
      Leia(ano)
      Escreva ("Em que ano voce nasceu? ")
      Leia (nasc)
      idade <- ano - nasc
      Escreva ("Em", ano, " voce tera ", idade, " anos.")
      se (idade >= 21) Entao
      Escreval (" e ja tera atigindo a maioridade ")
      FimSe
fimalgoritmo
